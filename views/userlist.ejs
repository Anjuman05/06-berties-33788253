<!doctype html>
<html>
  <head>
    <title>About <%=shopData.shopName%></title>
    <link rel="stylesheet"  type="text/css" href="/main.css" />
  </head>
  <body>
    <h1><%=shopData.shopName%></h1>
    <h2>Users</h2>
    <ul>
        <% users.forEach(function(user) {%>
            <li>
                <%= user.username %> - 
                <%= user.first_name %> <%= user.last_name %> -
                <%= user.email %>
            </li>
        <% }) %>
    </ul>
</body>
</html>